{
  "name": "aar-lang",
  "displayName": "aar lang",
  "description": "aardio...",
  "version": "0.0.1",
  "scripts": {
    "test": "nodemon -q --watch t.js --exec node t.js"
  },
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "keybindings": [
      {
        "key": "ctrl+r",
        "command": "aar.runAll",
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+shift+r",
        "command": "aar.runSelection",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceLangId == aar",
          "command": "aar.runAll",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "when": "resourceLangId == aar",
          "command": "aar.runAll",
          "group": "aar"
        },
        {
          "when": "resourceLangId == aar",
          "command": "aar.runSelection",
          "group": "aar"
        }
      ]
    },
    "commands": [
      {
        "command": "aar.runAll",
        "title": "运行此文件"
      },
      {
        "command": "aar.runSelection",
        "title": "运行所选"
      }
    ],
    "languages": [
      {
        "id": "aar",
        "aliases": [
          "aardio",
          "aar",
          "aau"
        ],
        "extensions": [
          ".aardio",
          ".aar",
          ".aau"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "aar",
        "scopeName": "source.aar",
        "path": "./syntaxes/aar.tmLanguage.json"
      }
    ]
  }
}
